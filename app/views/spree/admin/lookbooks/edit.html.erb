<% content_for :page_actions do %>
  <li><%= button_link_to Spree.t(:back_to_lookbooks_list), session[:return_to] || admin_lookbooks_url, :icon => 'arrow-left' %></li>
  <% if can?(:create, Spree::Product) %>
    <li id="new_product_link">
      <%= button_link_to Spree.t(:new_lookbook), new_object_url, { :icon => 'plus', :id => 'admin_new_lookbook' } %>
    </li>
  <% end %>
<% end %>

<%= render :partial => 'spree/admin/shared/content_sub_menu' %>

<%= render 'form' %>